{
  "extends": [
    "config:recommended"
  ],
  "configMigration": true,
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/(^|/)lazy-lock\.json$/"
      ],
      "matchStrings": [
        "\"(?<depName>[^\"]+)\": { \"branch\": \"(?<branch>[^\"]+)\", \"commit\": \"(?<currentValue>[^\"]+)\" }"
      ],
      "depNameTemplate": "https://github.com/{{depName}}/{{depName}}",
      "versioningTemplate": "git",
      "datasourceTemplate": "git-refs"
    }
  ],
  "git-submodules": {
    "enabled": true
  }
}
