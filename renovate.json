{
  "extends": [
    "config:base"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
	"lazy-lock.json$"
      ],
      "matchStrings": [
          "\"(?<depName>[^\"]+)\": { \"branch\": \"(?<branch>[^\"]+)\", \"commit\": \"(?<currentValue>[^\"]+)\" }"
      ],
      "datasourceTemplate": "git-submodules"
    }
  ],
  "git-submodules": {
    "enabled": true
  }
}
