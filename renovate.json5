{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  configMigration: true,
  packageRules: [
    {
      matchManagers: [
        'custom.regex',
      ],
      changelogUrl: "{{depName}}/compare/{{currentDigest}}...{{newDigest}}"
    },
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/(^|/)lazy-lock\\.json$/',
      ],
      matchStrings: [
        '"(?<depName>[^"]+)": { "branch": "(?<currentValue>[^"]+)", "commit": "(?<currentDigest>[^"]+)" }',
      ],
      depNameTemplate: "https://github.com/{{# if (equals depName 'blink.cmp') }}Saghen{{/if}}{{# if (equals depName 'copilot.vim') }}github{{/if}}{{# if (equals depName 'lazy.nvim') }}folke{{/if}}{{# if (equals depName 'lualine.nvim') }}nvim-lualine{{/if}}{{# if (equals depName 'mason-lspconfig.nvim') }}williamboman{{/if}}{{# if (equals depName 'mason.nvim') }}williamboman{{/if}}{{# if (equals depName 'nvim-lspconfig') }}neovim{{/if}}{{# if (equals depName 'nvim-tree.lua') }}nvim-tree{{/if}}{{# if (equals depName 'nvim-treesitter') }}nvim-treesitter{{/if}}{{# if (equals depName 'nvim-web-devicons') }}nvim-tree{{/if}}{{# if (equals depName 'nvim-web-devicons') }}nvim-tree{{/if}}{{# if (equals depName 'nvumi') }}josephburgess{{/if}}{{# if (equals depName 'rustaceanvim') }}mrcjkb{{/if}}{{# if (equals depName 'snacks.nvim') }}folke{{/if}}{{# if (equals depName 'trouble.nvim') }}folke{{/if}}/{{depName}}",
      versioningTemplate: 'git',
      datasourceTemplate: 'git-refs',
    },
  ],
  logLevelRemap: [
    {
      matchMessage: '/^Dependency extraction complete/',
      newLogLevel: 'trace',
    },
  ],
  'git-submodules': {
    enabled: true,
  },
}
