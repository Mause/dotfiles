{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  configMigration: true,
  packageRules: [
    {
      matchManagers: [
        'custom.regex',
      ],
      changelogUrl: '{{depName}}/compare/{{currentDigest}}...{{newDigest}}',
    },
    {
      matchDepNames: [
        'blink.indent',
        'blink.pairs',
        'blink.cmp',
      ],
      overrideDatasource: 'git-tags',
    },
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/(^|/)lazy-lock\\.json$/',
      ],
      matchStrings: [
        '"(?<depName>[^"]+)": { "branch": "(?<currentValue>[^"]+)", "commit": "(?<currentDigest>[^"]+)" }',
      ],
      depNameTemplate: "https://github.com/{{# if (equals depName 'Comment.nvim') }}numToStr{{/if}}\
{{# if (equals depName 'SchemaStore.nvim') }}b0o{{/if}}\
{{# if (equals depName 'aerial.nvim') }}stevearc{{/if}}\
{{# if (equals depName 'blink.cmp') }}Saghen{{/if}}\
{{# if (equals depName 'blink.download') }}saghen{{/if}}\
{{# if (equals depName 'blink.indent') }}saghen{{/if}}\
{{# if (equals depName 'blink.pairs') }}saghen{{/if}}\
{{# if (equals depName 'copilot.vim') }}github{{/if}}\
{{# if (equals depName 'git-notify.nvim') }}pynappo{{/if}}\
{{# if (equals depName 'lazy.nvim') }}folke{{/if}}\
{{# if (equals depName 'lualine.nvim') }}nvim-lualine{{/if}}\
{{# if (equals depName 'markview.nvim') }}OXY2DEV{{/if}}\
{{# if (equals depName 'mason-lspconfig.nvim') }}williamboman{{/if}}\
{{# if (equals depName 'mason.nvim') }}williamboman{{/if}}\
{{# if (equals depName 'none-ls.nvim') }}nvimtools{{/if}}\
{{# if (equals depName 'nui.nvim') }}MunifTanjim{{/if}}\
{{# if (equals depName 'nvim-dap') }}mfussenegger{{/if}}\
{{# if (equals depName 'nvim-dap-ui') }}rcarriga{{/if}}\
{{# if (equals depName 'nvim-lint') }}mfussenegger{{/if}}\
{{# if (equals depName 'nvim-lsp-notify') }}mrded{{/if}}\
{{# if (equals depName 'nvim-lspconfig') }}neovim{{/if}}\
{{# if (equals depName 'nvim-navic') }}SmiteshP{{/if}}\
{{# if (equals depName 'nvim-nio') }}nvim-neotest{{/if}}\
{{# if (equals depName 'nvim-notify') }}rcarriga{{/if}}\
{{# if (equals depName 'nvim-tree.lua') }}nvim-tree{{/if}}\
{{# if (equals depName 'nvim-treesitter') }}nvim-treesitter{{/if}}\
{{# if (equals depName 'nvim-web-devicons') }}nvim-tree{{/if}}\
{{# if (equals depName 'nvumi') }}josephburgess{{/if}}\
{{# if (equals depName 'octo.nvim') }}pwntester{{/if}}\
{{# if (equals depName 'pipeline.nvim') }}topaxi{{/if}}\
{{# if (equals depName 'plenary.nvim') }}nvim-lua{{/if}}\
{{# if (equals depName 'refactoring.nvim') }}ThePrimeagen{{/if}}\
{{# if (equals depName 'rustaceanvim') }}mrcjkb{{/if}}\
{{# if (equals depName 'smear-cursor.nvim') }}sphamba{{/if}}\
{{# if (equals depName 'snacks.nvim') }}folke{{/if}}\
{{# if (equals depName 'store.nvim') }}alex-popov-tech{{/if}}\
{{# if (equals depName 'telescope-fzf-native.nvim') }}nvim-telescope{{/if}}\
{{# if (equals depName 'telescope-ui-select.nvim') }}nvim-telescope{{/if}}\
{{# if (equals depName 'telescope.nvim') }}nvim-telescope{{/if}}\
{{# if (equals depName 'tiny-glimmer.nvim') }}rachartier{{/if}}\
{{# if (equals depName 'tokyonight.nvim') }}folke{{/if}}\
{{# if (equals depName 'triforce.nvim') }}gisketch{{/if}}\
{{# if (equals depName 'trouble.nvim') }}folke{{/if}}\
{{# if (equals depName 'volt') }}nvzone{{/if}}\
{{# if (equals depName 'which-key.nvim') }}folke{{/if}}/{{depName}}",
      versioningTemplate: 'git',
      datasourceTemplate: "{{# if (containsString 'blink.indent,blink.pairs,blink.cmp' depName) }}git-tags{{ else }}git-refs{{/if}}",
    },
  ],
  logLevelRemap: [
    {
      matchMessage: '/.*Dependency extraction complete.*/',
      newLogLevel: 'trace',
    },
  ],
  'git-submodules': {
    enabled: true,
  },
}
